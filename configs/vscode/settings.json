{
  "editor.minimap.enabled": false,
  "explorer.confirmDragAndDrop": false,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.exclude": {
    "**/__pycache__": true,
    "**/.mypy_cache/": true,
    "**/.pytest_cache": true,
    "**/.terraform": true,
    "**/.venv/": true,
    "**/*.pyc": true,
    "**/venv/": true
  },
  "ansibleVaultInline.executable": "/usr/local/bin/ansible-vault",
  "ansibleVaultInline.keyfile": "~/.ansible_vault.pass",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "files.trimFinalNewlines": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terraform.indexing": {
    "enabled": false,
    "liveIndexing": false,
    "delay": 500,
    "exclude": [".terraform/**/*", "**/.terraform/**/*"]
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.rulers": [79, 99],
  "python.showStartPage": false,
  "git.autofetch": true,
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "files.associations": {
    "*.tfvars": "terraform"
  },
  "workbench.colorTheme": "Monokai Dimmed",
  "python.formatting.provider": "black",
  "terminal.integrated.shell.osx": "/bin/zsh",
  "python.languageServer": "Pylance",
  "git.ignoreRebaseWarning": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "redhat.telemetry.enabled": false,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.startupEditor": "none",
  "security.workspace.trust.untrustedFiles": "open",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "jest.autoRun": "off",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "remote.SSH.defaultExtensions": ["gitpod.gitpod-remote-ssh"],
  "[dockercompose]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[sql]": {
    "editor.defaultFormatter": "henriblancke.vscode-dbt-formatter"
  },
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": true,
    "markdown": true
  },
  "java.import.gradle.java.home": "/opt/homebrew/Cellar/openjdk@11/11.0.15/libexec/openjdk.jdk/Contents/Home",
  "remote.SSH.configFile": "/var/folders/b_/q0twld_j77j3j_hqx_w36l3h0000gn/T/gitpod_ssh_config-86943-B37sG7XD3wGs",
  "yaml.schemas": {
    "https://squidfunk.github.io/mkdocs-material/schema.json": "mkdocs.yml"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "git.mergeEditor": false,
  "terraform.languageServer.enable": true
}
